{
  "name": "forge-framework",
  "version": "1.0.0",
  "description": "A lightweight frontend framework with tree shaking support",
  "type": "module",
  "main": "./dist/forge.cjs.js",
  "module": "./dist/forge.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/forge.esm.js",
      "require": "./dist/forge.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.esm.js",
      "require": "./dist/utils.cjs.js"
    },
    "./state": {
      "import": "./dist/state.esm.js",
      "require": "./dist/state.cjs.js"
    },
    "./dom": {
      "import": "./dist/dom.esm.js",
      "require": "./dist/dom.cjs.js"
    },
    "./component": {
      "import": "./dist/component.esm.js",
      "require": "./dist/component.cjs.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "framework",
    "frontend",
    "tree-shaking",
    "lightweight",
    "state-management"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.9.6",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
